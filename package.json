{
  "name": "promisify-all-proto",
  "version": "1.0.0",
  "description": "Promisify all including protoypes",
  "main": "dist/promisify-all.js",
  "module": "dist/promisify-all.es.js",
  "scripts": {
    "test": "npm run build && cross-env NODE_ENV=test ava",
    "prebuild": "rimraf dist",
    "build": "rollup -c"
  },
  "files": [
    "dist",
    "logo.jpg"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janryWang/promisify-all.git"
  },
  "author": "Allen Ng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/janryWang/promisify-all/issues"
  },
  "homepage": "https://github.com/janryWang/promisify-all#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.0.0-beta.42",
    "@babel/node": "7.0.0-beta.42",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "ava": "^1.0.0-beta.3",
    "cross-env": "^5.1.4",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "^1.11.x",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "4.0.0-beta.3"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --write --tab-width 2 --no-semi",
      "git add"
    ],
    "dist/*.js": [
      "prettier --write --tab-width 2 --no-semi",
      "git add"
    ],
    "test.js": [
      "prettier --write --tab-width 2 --no-semi",
      "git add"
    ]
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  }
}
